<title>UNIDENTIFIED VPN</title>
<script src="https://telegram.org/js/telegram-web-app.js"></script>
<style>
@import url('https://fonts.googleapis.com/css2?family=JetBrains+Mono:wght@300;400;500;600;700;800&family=Share+Tech+Mono&display=swap');
:root{
--bg:#0b0e14;--bg2:#0f1219;--card:#141821;--border:#1c2233;
--t1:#e2e8f0;--t2:#94a3b8;--t3:#64748b;--t4:#1e293b;
--grn:#22d45e;--grn-d:rgba(34,212,94,.10);--grn-l:#4ade80;
--tif:#0abab5;--tif-d:rgba(10,186,181,.10);--tif-l:#2dd4bf;
--neo:#f5c842;--neo-d:rgba(245,200,66,.10);--neo-l:#fbbf24;
--bronze:#cd7f32;--silver:#c0c0c0;--gold:#ffd700;--diamond:#b9f2ff;
--blue:#3b82f6;--purp:#a855f7;--pink:#ff2d7c;
--purp-neo:#bf5af2;--purp-neo-d:rgba(191,90,242,.10);--purp-neo-l:#d68fff;
}
*{margin:0;padding:0;box-sizing:border-box;-webkit-tap-highlight-color:transparent}
html,body{background:var(--bg);color:var(--t1);font-family:'JetBrains Mono',monospace;font-size:13px;line-height:1.5;overflow-x:hidden;min-height:100vh}
::-webkit-scrollbar{width:3px}::-webkit-scrollbar-thumb{background:var(--border);border-radius:2px}
.screen{display:none;flex-direction:column;min-height:100vh;padding:0 16px 90px;position:relative}.screen.active{display:flex}
.screen.glitch-out{display:flex}

/* Variant 1: Matrix breach */
.screen.in-v1{animation:inV1 .5s ease forwards}
.screen.out-v1{animation:outV1 .3s ease forwards}
@keyframes inV1{
0%{opacity:0;clip-path:inset(0 0 100% 0);filter:brightness(1.8) saturate(2) hue-rotate(80deg)}
8%{opacity:.3;clip-path:inset(0 0 85% 0);filter:brightness(1.6) saturate(1.8) hue-rotate(60deg);transform:translateX(-2px)}
16%{opacity:.3;clip-path:inset(0 0 70% 0);filter:brightness(1.5) saturate(1.6) hue-rotate(40deg);transform:translateX(3px)}
20%{opacity:.2;clip-path:inset(30% 0 50% 0);transform:translateX(-1px)}
28%{opacity:.5;clip-path:inset(0 0 50% 0);filter:brightness(1.3) hue-rotate(20deg);transform:translateX(2px)}
36%{opacity:.4;clip-path:inset(20% 0 30% 0);transform:translateX(-3px)}
44%{opacity:.7;clip-path:inset(0 0 25% 0);filter:brightness(1.1) hue-rotate(10deg);transform:translateX(1px)}
52%{opacity:.6;clip-path:inset(10% 0 15% 0);transform:translateX(-1px)}
64%{opacity:.85;clip-path:inset(0 0 8% 0);filter:brightness(1.05);transform:translateX(0)}
78%{opacity:.95;clip-path:inset(3% 0 0 0);filter:none;transform:translateY(-1px)}
100%{opacity:1;clip-path:inset(0);filter:none;transform:translate(0)}
}
@keyframes outV1{
0%{opacity:1;clip-path:inset(0);filter:none}
15%{opacity:.8;clip-path:inset(0 0 0 0);filter:brightness(1.3) hue-rotate(20deg);transform:translateX(2px)}
30%{opacity:.5;clip-path:inset(20% 0 30% 0);filter:brightness(1.5) hue-rotate(40deg);transform:translateX(-3px)}
50%{opacity:.3;clip-path:inset(40% 0 40% 0);filter:brightness(1.8) hue-rotate(60deg);transform:translateX(2px)}
70%{opacity:.15;clip-path:inset(60% 0 30% 0);filter:brightness(2) hue-rotate(80deg)}
100%{opacity:0;clip-path:inset(100% 0 0 0);filter:brightness(2.5) saturate(2) hue-rotate(90deg)}
}

/* Variant 5: Cyber flicker ‚Äî aggressive, daring */
.screen.in-v5{animation:inV5 .55s steps(1) forwards}
.screen.out-v5{animation:outV5 .25s steps(1) forwards}
@keyframes inV5{
0%{opacity:0;filter:none}
4%{opacity:1;filter:invert(1) hue-rotate(180deg) brightness(2);transform:translateX(8px) skewX(-3deg)}
8%{opacity:0}
12%{opacity:.9;filter:brightness(3) saturate(0);transform:translateX(-6px) scaleY(1.03)}
15%{opacity:0}
18%{opacity:.7;filter:invert(1) hue-rotate(90deg);clip-path:inset(20% 0 40% 0);transform:translateX(4px)}
22%{opacity:0}
26%{opacity:1;filter:hue-rotate(180deg) brightness(1.8);clip-path:inset(0);transform:translateX(-5px) skewX(2deg)}
29%{opacity:0;clip-path:inset(50% 0 10% 0)}
33%{opacity:.8;filter:invert(1) contrast(2);clip-path:inset(0);transform:translateY(-4px)}
36%{opacity:0}
40%{opacity:1;filter:brightness(2.5) hue-rotate(60deg) saturate(3);transform:translateX(3px) scaleX(1.02)}
43%{opacity:0}
47%{opacity:.9;filter:invert(1) brightness(1.5);clip-path:inset(30% 0 20% 0);transform:translateX(-3px)}
50%{opacity:1;filter:hue-rotate(40deg) brightness(1.3);clip-path:inset(0);transform:none}
54%{opacity:0}
58%{opacity:1;filter:brightness(1.8);transform:translateX(2px)}
61%{opacity:.5;filter:invert(1);transform:translateX(-2px)}
65%{opacity:1;filter:brightness(1.2) hue-rotate(15deg);transform:none}
70%{opacity:0}
73%{opacity:1;filter:none;transform:translateX(1px)}
78%{opacity:.8;filter:brightness(1.4)}
82%{opacity:1;filter:none}
88%{opacity:.9;filter:brightness(1.1);transform:translateX(-1px)}
92%{opacity:1;filter:none;transform:none}
100%{opacity:1;filter:none;transform:none;clip-path:inset(0)}
}
@keyframes outV5{
0%{opacity:1;filter:none;transform:none}
8%{opacity:0}
15%{opacity:.9;filter:invert(1) hue-rotate(180deg);transform:translateX(6px) skewX(-2deg)}
22%{opacity:0}
30%{opacity:.6;filter:brightness(3) saturate(0);clip-path:inset(20% 0 30% 0);transform:translateX(-5px)}
38%{opacity:0}
48%{opacity:.4;filter:invert(1) brightness(2);clip-path:inset(40% 0 20% 0);transform:translateY(3px)}
55%{opacity:0}
65%{opacity:.2;filter:brightness(3) hue-rotate(90deg);transform:scaleY(.95)}
75%{opacity:0}
85%{opacity:.1;filter:invert(1) brightness(2.5);transform:translateX(3px)}
100%{opacity:0}
}

/* Variant 6: Signal Intercept ‚Äî screen tears apart into bands, 
   each band shifts independently, RGB channels split, 
   then bands snap back together with static burst */
.screen.in-v6{animation:inV6 .6s cubic-bezier(.1,.7,.3,1) forwards}
.screen.out-v6{animation:outV6 .3s ease-in forwards}
@keyframes inV6{
0%{
  opacity:0;
  filter:blur(12px) brightness(3) saturate(0);
  transform:scale(1.08);
  clip-path:polygon(0 45%,100% 45%,100% 55%,0 55%)
}
5%{
  opacity:.6;
  filter:blur(8px) brightness(2) saturate(.5);
  clip-path:polygon(0 40%,100% 42%,100% 60%,0 58%);
  transform:scale(1.06) translateX(6px)
}
10%{
  opacity:.4;
  filter:blur(6px) brightness(2.5) saturate(0) hue-rotate(90deg);
  clip-path:polygon(0 30%,100% 35%,100% 70%,0 65%);
  transform:scale(1.04) translateX(-8px)
}
18%{
  opacity:.7;
  filter:blur(3px) brightness(1.6) saturate(2) hue-rotate(60deg);
  clip-path:polygon(0 15%,100% 20%,100% 80%,0 85%);
  transform:scale(1.02) translateX(4px)
}
24%{
  opacity:.5;
  filter:blur(5px) brightness(2) saturate(.3);
  clip-path:polygon(0 18%,100% 12%,100% 75%,0 82%);
  transform:translateX(-5px) skewX(2deg)
}
32%{
  opacity:.8;
  filter:blur(1px) brightness(1.3) hue-rotate(30deg);
  clip-path:polygon(0 5%,100% 8%,100% 92%,0 95%);
  transform:translateX(3px) skewX(-1deg)
}
40%{
  opacity:.6;
  filter:blur(3px) brightness(1.8) hue-rotate(45deg);
  clip-path:polygon(0 8%,100% 3%,100% 88%,0 97%);
  transform:translateX(-2px) skewX(1.5deg)
}
50%{
  opacity:.9;
  filter:blur(0) brightness(1.15) hue-rotate(15deg);
  clip-path:polygon(0 2%,100% 0,100% 98%,0 100%);
  transform:translateX(1px) skewX(-.5deg)
}
62%{
  opacity:.85;
  filter:brightness(1.4) hue-rotate(25deg) saturate(1.5);
  clip-path:inset(0);
  transform:translateX(-3px)
}
68%{
  opacity:1;
  filter:brightness(.85);
  transform:translateX(2px)
}
76%{
  filter:brightness(1.2) hue-rotate(8deg);
  transform:translateX(-1px)
}
84%{
  filter:brightness(.95);
  transform:translateX(0)
}
92%{
  filter:brightness(1.05);
}
100%{
  opacity:1;
  filter:none;
  transform:none;
  clip-path:inset(0)
}
}
@keyframes outV6{
0%{
  opacity:1;filter:none;transform:none;clip-path:inset(0)
}
12%{
  opacity:.9;
  filter:brightness(1.4) hue-rotate(10deg);
  transform:translateX(3px) skewX(1deg);
  clip-path:inset(0)
}
25%{
  opacity:.6;
  filter:blur(2px) brightness(1.8) hue-rotate(35deg) saturate(1.5);
  transform:translateX(-5px) skewX(-2deg);
  clip-path:polygon(0 10%,100% 5%,100% 85%,0 90%)
}
45%{
  opacity:.4;
  filter:blur(4px) brightness(2.2) hue-rotate(60deg) saturate(.5);
  transform:translateX(6px) skewX(2deg);
  clip-path:polygon(0 25%,100% 20%,100% 70%,0 75%)
}
70%{
  opacity:.2;
  filter:blur(8px) brightness(2.8) saturate(0) hue-rotate(80deg);
  transform:scale(1.05) translateX(-4px);
  clip-path:polygon(0 40%,100% 38%,100% 58%,0 60%)
}
100%{
  opacity:0;
  filter:blur(12px) brightness(3) saturate(0);
  transform:scale(1.08);
  clip-path:polygon(0 48%,100% 48%,100% 52%,0 52%)
}
}
@keyframes inV5{
0%{opacity:0}
10%{opacity:.8;filter:invert(1) hue-rotate(180deg)}
15%{opacity:0}
25%{opacity:.6;filter:brightness(2) hue-rotate(90deg);transform:translateY(-3px)}
30%{opacity:0}
40%{opacity:.9;filter:invert(1)}
45%{opacity:1;filter:none}
50%{opacity:0}
55%{opacity:1;filter:brightness(1.5)}
65%{opacity:1;filter:none;transform:translateY(0)}
100%{opacity:1;filter:none;transform:translateY(0)}
}
@keyframes outV5{
0%{opacity:1;filter:none}
20%{opacity:0}
35%{opacity:.7;filter:invert(1)}
45%{opacity:0}
60%{opacity:.3;filter:brightness(3)}
100%{opacity:0}
}
.screen>*{position:relative;z-index:1}
.scanlines{position:fixed;top:0;left:0;width:100%;height:100%;pointer-events:none;z-index:150;background:repeating-linear-gradient(0deg,transparent,transparent 2px,rgba(0,0,0,.035) 2px,rgba(0,0,0,.035) 4px)}
.matrix-rain{position:fixed;top:0;left:0;width:100vw;height:100vh;z-index:0;pointer-events:none;opacity:.18}

/* HEADER - boot text slightly different color */
.hdr{position:sticky;top:0;z-index:100;background:linear-gradient(180deg,rgba(11,14,20,.92) 60%,rgba(11,14,20,.4) 80%,transparent);backdrop-filter:blur(8px);-webkit-backdrop-filter:blur(8px);padding:16px 0 22px;margin:0 -16px;padding-left:16px;padding-right:16px}
.hdr-b{font-size:10px;font-family:'Share Tech Mono',monospace;letter-spacing:2px;margin-bottom:10px;text-align:left}
.hdr-b.cg{color:#1a7a3a}.hdr-b.cn{color:#8a6a1a}.hdr-b.ct{color:#0a6a67}.hdr-b.ck{color:#5a2a5a}.hdr-b.cp{color:#6a2a8a}
.hdr-t{font-size:28px;font-weight:800;letter-spacing:5px;text-transform:uppercase;display:inline-block}
.hdr-t.typing{border-right:2px solid currentColor;animation:titleBlink .5s step-end infinite}
.hdr-t.typed{border-right:2px solid transparent}
@keyframes titleBlink{0%,100%{border-color:currentColor}50%{border-color:transparent}}
.hdr-t.cg{color:var(--grn)}
.hdr-t.cn{color:var(--neo)}
.hdr-t.ct{color:var(--tif)}
.hdr-t.ck{color:var(--grn-l)}
.hdr-t.cp{color:var(--purp-neo)}
.hdr-t::after{content:'';display:none}
/* Soft glow behind title via pseudo element */
.hdr-wrap{position:relative;display:inline-block}
.hdr-glow{position:absolute;left:50%;top:50%;transform:translate(-50%,-50%);width:120%;height:200%;border-radius:50%;filter:blur(40px);opacity:.15;pointer-events:none;z-index:-1}
.hdr-glow.cg{background:var(--grn)}.hdr-glow.cn{background:var(--neo)}.hdr-glow.ct{background:var(--tif)}.hdr-glow.ck{background:var(--grn-l)}.hdr-glow.cp{background:var(--purp-neo)}
.hdr-s{font-size:12px;color:var(--t2);letter-spacing:4px;text-transform:uppercase;margin-top:6px;font-weight:600}
.gl{position:relative}.gl::before,.gl::after{content:attr(data-text);position:absolute;left:0;top:0;width:100%;height:100%}
.gl.cg::before{animation:g1 5s infinite;color:#ff0040;z-index:-1}.gl.cg::after{animation:g2 5s infinite;color:#0ff;z-index:-2}
.gl.cn::before{animation:g1 5s infinite;color:#ff2d55;z-index:-1}.gl.cn::after{animation:g2 5s infinite;color:#bf5af2;z-index:-2}
.gl.ct::before{animation:g1 5s infinite;color:#ff6b00;z-index:-1}.gl.ct::after{animation:g2 5s infinite;color:#00ff9d;z-index:-2}
.gl.ck::before{animation:g1 5s infinite;color:#ff4060;z-index:-1}.gl.ck::after{animation:g2 5s infinite;color:#40ffff;z-index:-2}
.gl.cp::before{animation:g1 5s infinite;color:#ff2d55;z-index:-1}.gl.cp::after{animation:g2 5s infinite;color:#00e5ff;z-index:-2}
@keyframes g1{0%,96%,100%{clip-path:inset(0);transform:translate(0)}97%{clip-path:inset(20% 0 50% 0);transform:translate(-2px,1px)}99%{clip-path:inset(60% 0 10% 0);transform:translate(2px,-1px)}}
@keyframes g2{0%,94%,100%{clip-path:inset(0);transform:translate(0)}95%{clip-path:inset(30% 0 30% 0);transform:translate(2px,-1px)}98%{clip-path:inset(50% 0 20% 0);transform:translate(-1px,2px)}}

.sbar{display:flex;align-items:center;gap:14px;padding:10px 0;border-bottom:1px solid var(--border);font-size:10px;color:var(--t2);overflow-x:auto;white-space:nowrap}
.sdot{width:7px;height:7px;border-radius:50%;background:var(--grn);box-shadow:0 0 10px var(--grn);animation:pu 2s infinite;flex-shrink:0}
@keyframes pu{0%,100%{opacity:1}50%{opacity:.3}}.si{display:flex;align-items:center;gap:5px}.sl{color:var(--t3)}

.sh{display:flex;align-items:center;gap:10px;margin:24px 0 14px;font-size:12px;color:var(--t2);letter-spacing:3px;text-transform:uppercase;font-weight:700}
.sh::after{content:'';flex:1;height:1px;background:linear-gradient(90deg,var(--border),transparent);animation:lineGrow .8s ease forwards;transform-origin:left;transform:scaleX(0)}
@keyframes lineGrow{to{transform:scaleX(1)}}
.sh.neo{color:var(--neo)}.sh.tif{color:var(--tif)}.sh.keys{color:var(--grn-l)}.sh.purp{color:var(--purp-neo)}

/* ACTIVE SUB */
.asub{background:var(--card);border:1px solid #2ee86a;border-radius:12px;padding:16px;margin-top:14px;animation:subPulse 3s ease-in-out infinite}
@keyframes subPulse{0%,100%{box-shadow:0 0 0px rgba(34,212,94,.05);border-color:rgba(46,232,106,.3)}50%{box-shadow:0 0 20px rgba(34,212,94,.12);border-color:rgba(46,232,106,.7)}}
.as-top{display:flex;justify-content:space-between;align-items:center;margin-bottom:8px}
.as-nm{font-size:16px;font-weight:800;color:#2ee86a}
.as-bg{font-size:9px;padding:3px 10px;border-radius:4px;background:var(--grn-d);color:var(--grn);border:1px solid var(--grn);font-weight:700;letter-spacing:1px}
.as-ex{font-size:10px;color:var(--t3);margin-bottom:10px}
/* Key type tabs */
.key-tabs{display:flex;gap:6px;margin-bottom:10px}
.key-tab{flex:1;padding:10px;text-align:center;background:var(--bg);border:1px solid var(--border);border-radius:8px;cursor:pointer;font-family:'JetBrains Mono',monospace;font-size:12px;font-weight:800;color:var(--t1);letter-spacing:1px;transition:all .3s ease}
.key-tab.on{border-color:#2ee86a;color:#2ee86a;background:rgba(46,232,106,.08);box-shadow:inset 0 0 12px rgba(46,232,106,.06)}
#scrKeys .key-tab.on{border-color:var(--purp-neo);color:var(--purp-neo);background:var(--purp-neo-d);box-shadow:inset 0 0 12px rgba(191,90,242,.06)}
#scrKeys .as-k .kv{color:#c878f0}
#scrKeys .cp-btn:active{background:var(--purp-neo-d);border-color:var(--purp-neo);color:var(--purp-neo)}
#scrKeys .as-bg{background:var(--purp-neo-d);color:var(--purp-neo);border-color:var(--purp-neo)}
#scrKeys .asub{border-color:var(--purp-neo);animation:subPulsePurp 3s ease-in-out infinite}
@keyframes subPulsePurp{0%,100%{box-shadow:0 0 0px rgba(191,90,242,.05);border-color:rgba(191,90,242,.3)}50%{box-shadow:0 0 20px rgba(191,90,242,.12);border-color:rgba(191,90,242,.7)}}
#scrKeys .as-nm{color:var(--purp-neo-l)}
.key-tab:active{transform:scale(.96)}
.key-tab.on{border-color:#2ee86a;color:#2ee86a;background:rgba(46,232,106,.08)}
.key-tab svg{width:14px;height:14px;stroke:currentColor;fill:none;stroke-width:2;vertical-align:middle;margin-right:4px}
.as-k{background:var(--bg);border:1px solid var(--border);border-radius:8px;padding:12px;font-size:10px;position:relative;word-break:break-all}
.as-k .kl{color:var(--t3);margin-bottom:4px;font-size:9px;letter-spacing:1px}.as-k .kv{color:var(--grn);font-weight:600}
.cp-btn{position:absolute;top:8px;right:8px;background:var(--card);border:1px solid var(--border);border-radius:6px;padding:6px 8px;cursor:pointer;color:var(--t2);display:flex;align-items:center}
.cp-btn:active{background:var(--grn-d);border-color:var(--grn);color:var(--grn);animation:copyPop .3s ease}
@keyframes copyPop{0%{transform:scale(1)}50%{transform:scale(1.25)}100%{transform:scale(1)}}
.cp-btn svg{width:14px;height:14px;stroke:currentColor;fill:none;stroke-width:2}

/* PLANS */
.cur-sw{display:flex;gap:4px;margin-bottom:14px}
.cur-b{flex:1;padding:10px;text-align:center;background:var(--bg);border:1px solid var(--border);border-radius:8px;cursor:pointer;font-family:'JetBrains Mono',monospace;font-size:16px;font-weight:800;color:var(--t3);letter-spacing:1.5px;transition:all .25s ease;display:flex;align-items:center;justify-content:center;gap:6px}
.cur-b img{filter:grayscale(1) brightness(.45) contrast(.8);opacity:.55;transition:all .25s ease}
.cur-b.on img{filter:grayscale(0) brightness(1) contrast(1);opacity:1}
.cur-b:active{transform:scale(.95)}
.cur-b.on[data-c="USDT"]{border-color:#20d4a0;box-shadow:0 0 12px rgba(32,212,160,.15),inset 0 0 0 100px rgba(32,212,160,.08);color:transparent;-webkit-text-fill-color:transparent;background:linear-gradient(90deg,#20d4a0,#3dbf70,#20d4a0);background-size:200% 100%;-webkit-background-clip:text;background-clip:text;animation:priceShimmer 4s ease-in-out infinite}
.cur-b.on[data-c="RUB"]{border-color:#3dbf70;box-shadow:0 0 12px rgba(61,191,112,.15),inset 0 0 0 100px rgba(61,191,112,.08);color:transparent;-webkit-text-fill-color:transparent;background:linear-gradient(90deg,#3dbf70,#7ab844,#3dbf70);background-size:200% 100%;-webkit-background-clip:text;background-clip:text;animation:priceShimmer 4s ease-in-out infinite}
@keyframes priceGlitch{
0%{opacity:1;transform:translate(0);clip-path:inset(0)}
10%{opacity:.8;transform:translate(-3px,1px);clip-path:inset(20% 0 40% 0);text-shadow:2px 0 #ff0040,-2px 0 #0ff}
20%{opacity:.6;transform:translate(2px,-1px);clip-path:inset(60% 0 5% 0);text-shadow:-2px 0 #ff0040,2px 0 #0ff}
30%{opacity:.4;transform:translate(-1px,2px);clip-path:inset(10% 0 70% 0);text-shadow:1px 0 #ff0040,-1px 0 #0ff}
40%{opacity:0;transform:translate(3px,0);clip-path:inset(40% 0 20% 0)}
60%{opacity:0;transform:translate(-2px,-1px);clip-path:inset(50% 0 30% 0)}
70%{opacity:.5;transform:translate(1px,1px);clip-path:inset(15% 0 60% 0);text-shadow:-2px 0 #ff0040,2px 0 #0ff}
80%{opacity:.7;transform:translate(-1px,0);clip-path:inset(70% 0 10% 0);text-shadow:1px 0 #ff0040,-1px 0 #0ff}
90%{opacity:.9;transform:translate(1px,-1px);clip-path:inset(0);text-shadow:0 0 transparent}
100%{opacity:1;transform:translate(0);clip-path:inset(0);text-shadow:none}
}
.ppr.flip{animation:priceGlitch .4s ease}
.plans{display:flex;flex-direction:column;gap:10px}
.plan{animation:planSlide .4s ease both}
.plan:nth-child(1){animation-delay:.05s}
.plan:nth-child(2){animation-delay:.12s}
.plan:nth-child(3){animation-delay:.19s}
.plan:nth-child(4){animation-delay:.26s}
.plan:nth-child(5){animation-delay:.33s}
@keyframes planSlide{from{opacity:0;transform:translateX(-12px)}to{opacity:1;transform:translateX(0)}}

.plan{background:var(--card);border:1px solid var(--border);border-radius:12px;padding:18px;cursor:pointer;transition:all .15s}
.plan:active{transform:scale(.98);border-color:rgba(32,212,160,.5);box-shadow:0 0 20px rgba(32,212,160,.1)}
.plan-top{display:flex;justify-content:space-between;align-items:center;margin-bottom:2px}
