// MATRIX
let mxC='rgba(34,212,94,0.7)';let mxGrad=null;
function updateMC(){mxGrad=null;if(curScr==='scrRef')mxC='rgba(245,200,66,0.7)';else if(curScr==='scrProf')mxC='rgba(10,186,181,0.7)';else if(curScr==='scrKeys'){mxC='rgba(191,90,242,0.7)';mxGrad=['rgba(191,90,242,0.7)','rgba(200,80,210,0.7)','rgba(210,70,190,0.7)','rgba(220,60,170,0.7)','rgba(191,90,242,0.7)'];}else mxC='rgba(34,212,94,0.7)'}
function initMatrix(){const cv=document.getElementById('mxC');if(!cv)return;const ctx=cv.getContext('2d');const dpr=window.devicePixelRatio||1;
  function rz(){const w=window.innerWidth,h=window.innerHeight;cv.style.width=w+'px';cv.style.height=h+'px';cv.width=w*dpr;cv.height=h*dpr;ctx.setTransform(dpr,0,0,dpr,0,0);ctx.fillStyle='rgba(11,14,20,1)';ctx.fillRect(0,0,w,h)}
  rz();window.addEventListener('resize',rz);
  const fs=14;const cols=Math.floor(window.innerWidth/fs);let dr=[];for(let i=0;i<cols;i++)dr[i]=Math.floor(Math.random()*(window.innerHeight/fs));
  const ch='01„Ç¢„Ç§„Ç¶„Ç®„Ç™„Ç´„Ç≠„ÇØ„Ç±„Ç≥„Çµ„Ç∑„Çπ„Çª„ÇΩ„Çø„ÉÅ„ÉÑ„ÉÜ„Éà„Éä„Éã„Éå„Éç„Éé{}[]<>/|=+-*&^%$#@!?';
  function draw(){const w=window.innerWidth,h=window.innerHeight;ctx.fillStyle='rgba(11,14,20,0.04)';ctx.fillRect(0,0,w,h);
    ctx.font=fs+'px Share Tech Mono';const nc=Math.floor(w/fs);
    while(dr.length<nc)dr.push(Math.floor(Math.random()*h/fs));
    for(let i=0;i<nc;i++){if(mxGrad){ctx.fillStyle=mxGrad[i%mxGrad.length]}else{ctx.fillStyle=mxC}ctx.globalAlpha=.3+Math.random()*.7;ctx.fillText(ch[Math.floor(Math.random()*ch.length)],i*fs,dr[i]*fs);
      if(dr[i]*fs>h&&Math.random()>.975)dr[i]=0;dr[i]++}ctx.globalAlpha=1}
  if(window._mI)clearInterval(window._mI);window._mI=setInterval(draw,45)}

