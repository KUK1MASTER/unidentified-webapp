  el.dataset.typed='1';
  let i=0;
  const iv=setInterval(()=>{
    el.textContent+=text[i];
    i++;
    if(i>=text.length){
      clearInterval(iv);
      setTimeout(()=>{el.classList.remove('typing');el.classList.add('typed')},800);
    }
  },70);
}

